{
  "name": "@argodevops/semantic-release-rpm",
  "version": "1.0.2",
  "description": "plugin for semantic release to build and publish a rpm",
  "source": "src/index.js",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest --runInBand",
    "lint": "eslint \"./src/**\" --fix",
    "pretty": "prettier --write \"src/**/*.{js*,ts}\"",
    "prepare": "is-ci || husky install",
    "coverage": "node ./coverage-server.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argodevops/semantic-release-rpm.git"
  },
  "keywords": [
    "semantic-release"
  ],
  "author": "kevin.c@argodevops.co.uk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/argodevops/semantic-release-rpm/issues"
  },
  "homepage": "https://github.com/argodevops/semantic-release-rpm#readme",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@argodevops/eslint-config": "^1.2.2",
    "@argodevops/semantic-release-teams": "1.0.24",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@parcel/config-default": "^2.9.3",
    "@parcel/transformer-js": "^2.9.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/jest": "^29.5.3",
    "axios-mock-adapter": "^1.21.5",
    "babel-jest": "^29.6.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.6.2",
    "parcel": "^2.9.3",
    "semantic-release": "^21.0.7",
    "semantic-release-npm-deprecate": "^1.0.34"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@argodevops/semantic-release-rpm": "1.0.2",
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/git": "^10.0.1"
  },
  "pnpm": {
    "overrides": {
      "json5@<2.2.2": ">=2.2.2",
      "debug@<3.1.0": ">=3.1.0",
      "semver@<7.5.2": ">=7.5.2"
    }
  }
}
